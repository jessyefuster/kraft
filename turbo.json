{
  "globalDotEnv": [".env"],
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "type-check": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "test": {
      "dotEnv": [".env.test"],
      "outputs": []
    },
    "build": {
      "dependsOn": ["type-check", "lint"],
      "outputs": ["build/**", "dist/**"]
    },
    "migration:run": {
      "outputs": [],
      "cache": false
    },
    "migration:generate": {
      "outputs": [],
      "cache": false
    },
    "migration:revert": {
      "outputs": [],
      "cache": false
    },
    "migration:create": {
      "outputs": [],
      "cache": false
    }
  }
}